{
    "name": "snapcloud",
    "version": "1.0.0",
    "scripts": {
        "serve": "./Run.sh", 
        "buildnrun": "./BuildnRun.sh",
        "git:add": "git add .",
        "git:commit": "git commit -m 'deploy'",
        "git:push": "git push",
        "deploy": "npm run git:add && npm run git:commit && npm run git:push && caprover deploy --default",
        "fetch:backup": "ssh $REMOTE_SERVER './backup_snapcloud.sh'",
        "fetch:store": "rsync -avz --progress $REMOTE_STORE_PATH . && rm -rf ./store && mv ./snapcloud.app.store ./store",
        "fetch:db": "rsync -avz --progress $REMOTE_DB_PATH .",
        "fetch": "npm run fetch:backup && npm run fetch:store && npm run fetch:db"
    },
    "dependencies": {
    },
    "devDependencies": {
    }
}
